{
  "name": "Website dark mode",
  "description": "Dark theme for the Scratch website, both for 2.0 and 3.0 pages. Multiple options by different authors available.",
  "tags": ["community", "theme"],
  "credits": [
    {
      "name": "Maximouse (Experimental Dark)",
      "link": "https://github.com/mxmou/customize-scratch"
    }
  ],
  "dynamicEnable": true,
  "dynamicDisable": true,
  "injectAsStyleElt": true,
  "userscripts": [
    {
      "url": "experimental_forums_preview.js",
      "matches": [
        "https://localhost:8333/discuss/topic/*",
        "https://localhost:8333/discuss/post/*/edit/",
        "https://localhost:8333/discuss/settings/*/"
      ]
    }
  ],
  "userstyles": [
    {
      "url": "scrollbar.css",
      "matches": ["https://localhost:8333/*"]
    },
    {
      "url": "experimental_scratchr2.css",
      "matches": ["https://localhost:8333/*"],
      "settingMatch": {
        "id": "selectedMode",
        "value": "experimental-dark"
      }
    },
    {
      "url": "experimental_scratchwww.css",
      "matches": [
        "https://localhost:8333/",
        "https://localhost:8333/projects/*/",
        "https://localhost:8333/studios-playground/*",
        "https://localhost:8333/projects/*/editor/",
        "https://localhost:8333/projects/*/fullscreen/",
        "https://localhost:8333/explore/*",
        "https://localhost:8333/search/*",
        "https://localhost:8333/ideas/",
        "https://localhost:8333/about/",
        "https://localhost:8333/messages/",
        "https://localhost:8333/parents/",
        "https://localhost:8333/educators/",
        "https://localhost:8333/developers/",
        "https://localhost:8333/credits/",
        "https://localhost:8333/community_guidelines/",
        "https://localhost:8333/info/faq/",
        "https://localhost:8333/download/",
        "https://localhost:8333/contact-us/",
        "https://localhost:8333/terms_of_use/",
        "https://localhost:8333/privacy_policy/",
        "https://localhost:8333/DMCA/",
        "https://localhost:8333/conference/",
        "https://localhost:8333/scratch_1.4",
        "https://localhost:8333/sec",
        "https://localhost:8333/research",
        "https://localhost:8333/conference/*",
        "https://localhost:8333/annual-report",
        "https://localhost:8333/camp",
        "https://localhost:8333/join",
        "https://localhost:8333/starter-projects",
        "https://localhost:8333/classes/complete_registration",
        "https://localhost:8333/educators/faq",
        "https://localhost:8333/educators/register",
        "https://localhost:8333/educators/waiting",
        "https://localhost:8333/wedo",
        "https://localhost:8333/wedo-legacy",
        "https://localhost:8333/ev3",
        "https://localhost:8333/microbit",
        "https://localhost:8333/vernier",
        "https://localhost:8333/boost"
      ],
      "settingMatch": {
        "id": "selectedMode",
        "value": "experimental-dark"
      }
    },
    {
      "url": "experimental_forums.css",
      "matches": ["https://localhost:8333/discuss/*"],
      "settingMatch": {
        "id": "selectedMode",
        "value": "experimental-dark"
      }
    },
    {
      "url": "experimental_profile.css",
      "matches": ["https://localhost:8333/users/*", "https://localhost:8333/classes/*"],
      "settingMatch": {
        "id": "selectedMode",
        "value": "experimental-dark"
      }
    },
    {
      "url": "experimental_studio.css",
      "matches": ["https://localhost:8333/studios-playground/*"],
      "settingMatch": {
        "id": "selectedMode",
        "value": "experimental-dark"
      }
    },
    {
      "url": "experimental_scratchr2_comments.css",
      "matches": ["https://localhost:8333/users/*", "https://localhost:8333/studios-playground/*"],
      "settingMatch": {
        "id": "selectedMode",
        "value": "experimental-dark"
      }
    },
    {
      "url": "experimental_mystuff.css",
      "matches": ["https://localhost:8333/mystuff/"],
      "settingMatch": {
        "id": "selectedMode",
        "value": "experimental-dark"
      }
    },
    {
      "url": "experimental_settings.css",
      "matches": [
        "https://localhost:8333/accounts/settings/",
        "https://localhost:8333/accounts/password_change/",
        "https://localhost:8333/accounts/email_change/"
      ],
      "settingMatch": {
        "id": "selectedMode",
        "value": "experimental-dark"
      }
    },
    {
      "url": "experimental_statistics.css",
      "matches": ["https://localhost:8333/statistics/"],
      "settingMatch": {
        "id": "selectedMode",
        "value": "experimental-dark"
      }
    },
    {
      "url": "experimental_quotes.css",
      "matches": ["https://localhost:8333/info/quotes/"],
      "settingMatch": {
        "id": "selectedMode",
        "value": "experimental-dark"
      }
    },
    {
      "url": "experimental_project_embed.css",
      "matches": ["https://localhost:8333/projects/*/embed"],
      "settingMatch": {
        "id": "selectedMode",
        "value": "experimental-dark"
      }
    },
    {
      "url": "pygments.css",
      "matches": ["https://localhost:8333/discuss/topic/*"]
    }
  ],
  "settings": [
    {
      "name": "Dark mode version",
      "id": "selectedMode",
      "type": "select",
      "potentialValues": [
        {
          "id": "experimental-dark",
          "name": "\"Experimental\" Dark"
        }
      ],
      "default": "experimental-dark"
    }
  ],
  "l10n": true
}
