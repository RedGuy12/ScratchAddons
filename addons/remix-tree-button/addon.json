{
  "name": "Remix tree button on project pages",
  "description": "Brings back the remix tree button to project pages.",
  "tags": ["community"],
  "credits": [
    {
      "name": "Ucrash",
      "link": "https://github.com/gducrash"
    }
  ],
  "userscripts": [
    {
      "url": "main.js",
      "matches": ["https://localhost:8333/projects/*"],
      "runAtComplete": false
    }
  ],
  "userstyles": [
    {
      "url": "button.css",
      "matches": ["https://localhost:8333/projects/*"]
    }
  ],
  "settings": [
    {
      "name": "Button Color",
      "id": "buttonColor",
      "type": "color",
      "default": "#4d97ff"
    }
  ],
  "l10n": true
}
